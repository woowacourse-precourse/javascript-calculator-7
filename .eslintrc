{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": [
      "error",
      "always",
      {
        "js": "always",
      },
    ],
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
  },
}
