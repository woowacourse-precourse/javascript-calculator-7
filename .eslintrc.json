{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "class-methods-use-this": "off",
    "import/extensions": "always"
  }
}
