{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": ["off"],
    "import/no-unresolved": [
      "error",
      {
        "caseSensitive": false
      }
    ]
  }
}
